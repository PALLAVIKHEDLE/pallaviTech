<footer class="footer h-card" role="contentinfo">
    <div class="footer__inner container">
        <p class="footer__item">
            <strong>&copy; 2016&ndash;{{ build.timestamp | dateToFormat('yyyy') }}
            <a class="p-name u-url" rel="me" href="{{ site.url }}">{{ author.name }}</a></strong>
        </p>
        <p class="footer__item footer__item--right">
            <a class="footer__icon" href="{{ author.social.github.url }}"> {% icon "github" %}</a>
            <a class="footer__icon" href="{{ author.social.leetcode.url }}">{% icon "leetcode" %}</a>
             <a class="footer__icon" href="{{ author.social.linkedin.url }}">{% icon "linkedin" %}</a>
            <a class="footer__icon" href="{{ author.social.medium.url }}">{% icon "medium" %}</a>
        </p>

    </div>
</footer>